{
	"rewrites": [
		{
			"source": "/dashboard",
			"destination": "/index.html"
		}
	],
	"builds": [
		{
			"src": "frontend/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "frontend/build"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "frontend/build/$1"
		}
	]
}
